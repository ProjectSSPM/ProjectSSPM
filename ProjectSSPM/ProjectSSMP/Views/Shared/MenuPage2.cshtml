<ul id="gn-menu" class="gn-menu-main">
    <li class="gn-trigger">
        <a class="gn-icon gn-icon-menu"><span>Menu</span></a>
        <nav class="gn-menu-wrapper">
            <div class="gn-scroller">
                <ul class="gn-menu">
                    @{ 
                        var userMenu = ViewBag.userMenu;
                        foreach (var data in userMenu)
                        {
                            <li>
                                <a href="@data.MenuUrl" class="@data.MenuIcon">@data.MenuName</a>
                            </li>


                        }
                    }
                    
                    @*<li>
                        <a class="gn-icon gn-icon-pencil">Settings</a>
                    </li>*@
                    
                    
                </ul>
            </div>
            <!-- /gn-scroller -->
        </nav>
    </li>
    
    <li>
        <a class="codrops-icon codrops-icon-drop" asp-controller="Security" asp-action="Logout" ><span>logout</span></a>
    </li>
</ul>