@model IEnumerable<ProjectSSMP.Models.Timesheet.TimeSheetInputModel>
@{
    ViewData["Title"] = "TimeSheet Page";
    Layout = "~/Views/Shared/_LayoutMenu.cshtml";
}
<div class="container">
<h2 class="well">TimeSheet</h2>
 <table id="Timesheet" class="responsive" width="100%">
    <thead>
        <tr>
               
                <th>
                    Task Name
                </th>

                <th>
                    Function Name
                </th>

                 <th>
                    
                 
                   
                </th>
        </tr>
    </thead>
    <tbody>
        @{ 
            var timesheet = Model.GroupBy(f => f.FunctionId);
        }
        
@foreach (var item in Model) {
        <tr>
            
            <td>
              
                
            </td>


            <td>
                
                @Html.DisplayFor(modelItem => item.FunctionName)
            </td>



                
            <td>
                <div class= "text-right" style="margin-right: 10px;">
                    <a asp-action="AddTimeSheet" class="btn btn-info btn-sm" asp-route-id="@item.FunctionId">Add</a>  
                    <a asp-action="UpdateTimeSheet" class="btn btn-success btn-sm" asp-route-id="@item.FunctionId">Update</a> 
                </div>

            </td>
        </tr>

}
    </tbody>
    </table>

</div>